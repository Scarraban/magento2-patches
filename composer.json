{
    "name": "space48/magento2-patches",
    "description": "A collection of patches for core Magento2",
    "authors": [
        {
            "name": "DanCarlyon",
            "email": "dan.carlyon@space48.com"
        }
    ],
    "require": {
        "netresearch/composer-patches-plugin": "^1.2.1"
    },
    "extra": {
        "patches": {
            "magento/framework": {
                "100.*": [
                    {
